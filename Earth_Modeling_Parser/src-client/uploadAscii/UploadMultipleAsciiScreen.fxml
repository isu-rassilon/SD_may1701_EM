      <ProgressBar fx:id="progressBar" layoutX="1030.0" layoutY="520.0" prefWidth="200.0" progress="0.0" visible="false" />
      <Text fx:id="progressText" layoutX="1030.0" layoutY="512.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Progress:" visible="false" />
	</children>
