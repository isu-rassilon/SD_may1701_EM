<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox prefHeight="811.0" prefWidth="1250.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uploadAscii.UploadMultipleAsciiScreenController">
   <children>
      <HBox prefHeight="441.0" prefWidth="1184.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="75.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
         <children>
            <TextArea fx:id="messageTextArea" editable="false" prefHeight="309.0" prefWidth="1119.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="55.0" left="20.0" right="20.0" top="15.0" />
               </HBox.margin>
            </TextArea>
         </children>
      </HBox>
      <Button fx:id="selectFilesBtn" mnemonicParsing="false" prefWidth="140.0" text="Select File(s) To Upload" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="180.0" top="-45.0" />
         </VBox.margin>
      </Button>
      <Text fx:id="progressText" strokeType="OUTSIDE" strokeWidth="0.0" text="Progress:" visible="false" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="686.0" top="-20.0" />
         </VBox.margin>
      </Text>
      <Button fx:id="sendToServerBtn" mnemonicParsing="false" text="Send to Server" textAlignment="CENTER" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="686.0" />
         </VBox.margin>
      </Button>
      <VBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1250.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="backBtn" alignment="BASELINE_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="2.0" prefWidth="216.0" text="Back to manual upload page.">
               <graphic>
                  <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" visible="false" />
               </graphic>
               <VBox.margin>
                  <Insets left="686.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
