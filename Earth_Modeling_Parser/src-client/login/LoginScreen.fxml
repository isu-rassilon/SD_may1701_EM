<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="login.LoginScreenController">
	<children>
		<Text strokeType="OUTSIDE" strokeWidth="0.0"
			text="Visual Earth Modelling System" textAlignment="CENTER">
			<font>
				<Font name="System Bold" size="48.0" />
			</font>
			<VBox.margin>
				<Insets top="100.0" />
			</VBox.margin>
		</Text>
		<HBox alignment="BOTTOM_CENTER" prefHeight="0.0" prefWidth="1280.0"
			VBox.vgrow="ALWAYS">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username:"
					HBox.hgrow="ALWAYS">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextField fx:id="usernameTextField"
					prefHeight="0.0" prefWidth="171.0">
					<HBox.margin>
						<Insets left="50.0" />
					</HBox.margin>
				</TextField>
			</children>
			<VBox.margin>
				<Insets />
			</VBox.margin>
		</HBox>
		<HBox alignment="CENTER" prefHeight="0.0" prefWidth="1280.0"
			VBox.vgrow="ALWAYS">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password:"
					HBox.hgrow="ALWAYS">
					<font>
						<Font size="18.0" />
					</font>
					<HBox.margin>
						<Insets />
					</HBox.margin>
					<VBox.margin>
						<Insets left="60.0" />
					</VBox.margin>
				</Text>
				<PasswordField fx:id="passwordField">
					<HBox.margin>
						<Insets left="50.0" />
					</HBox.margin>
				</PasswordField>
			</children>
		</HBox>
		<Button fx:id="loginButton" mnemonicParsing="false"
			text="Login">
			<VBox.margin>
				<Insets bottom="150.0" left="260.0" top="-50.0" />
			</VBox.margin>
		</Button>
	</children>
</VBox>
